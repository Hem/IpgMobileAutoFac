<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Sample: @RenderSection("title", required: false)</title>

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/site.css" />

</head>
<body>

    <div class="navbar-header">

        <h3>Title</h3>

        <ipg-nav></ipg-nav>
    </div>

    <div class="container">
        @RenderBody()
    </div>

    <footer>
        <p>&copy; @DateTime.Now.Year - Sample Application</p>
    </footer>



    @Html.Partial("DefineAngular")
    <script>
        System.import('app/navigation/nav.module');
    </script>
    @RenderSection("scripts", required: false)
    

</body>
</html>
